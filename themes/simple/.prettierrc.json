{
    "trailingComma": "all",
    "semi": true,
    "singleQuote": false,
    "plugins": ["prettier-plugin-go-template", "prettier-plugin-tailwindcss"],
    "tailwindConfig": "./tailwind.config.js",
    "overrides": [
        {
            "files": ["*.html"],
            "options": {
                "parser": "go-template"
            }
        }
    ]
}
